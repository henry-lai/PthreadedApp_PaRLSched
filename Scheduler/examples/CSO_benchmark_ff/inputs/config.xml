<?xml version="1.0" encoding="utf-8"?>
<Inputs>
  <Run>Hard28_BPP900</Run>
  <DatabaseName>MeasDB</DatabaseName>
  <DatabaseServer>sibonga</DatabaseServer>
  <PowerLossFormula>./Formulas/DefaultFormulaPL+CM17T_averagesOnly_PskAvg1750.xml</PowerLossFormula>
  <NoiseLevelFormula>./Formulas/DefaultFormulaNL.xml</NoiseLevelFormula>
  <OutputDirectoryName>./results</OutputDirectoryName>
  <OptimizerName>CSO</OptimizerName>
  <ParallelDegree>40</ParallelDegree>
  <ApplyPaRLSched>1</ApplyPaRLSched>
  <RunSimulationTool>0</RunSimulationTool>
  <OptimizationParameters>
    <IncludePowerLossConstraints>0</IncludePowerLossConstraints>
    <IncludeNoiseLevelConstraints>0</IncludeNoiseLevelConstraints>
    <IncludeQualityConstraints>0</IncludeQualityConstraints>
    <ProbMutations>0.02</ProbMutations>
    <ProbNonAdmissibleOperations>0.02</ProbNonAdmissibleOperations>
    <MaxNumberPerturbations>100</MaxNumberPerturbations>
    <ProbRandomMode>0.02</ProbRandomMode>
    <ConstraintProcessingSteps>400</ConstraintProcessingSteps>
    <RandomModeProcessingSteps>2000</RandomModeProcessingSteps>
    <BetterReplyModeProcessingSteps>70</BetterReplyModeProcessingSteps>
    <BetterReplyModeInternalSteps>7</BetterReplyModeInternalSteps>
    <BetterReplyModeMaxRestBandCriterion>1</BetterReplyModeMaxRestBandCriterion>
    <SigmoidParameter>0.1</SigmoidParameter>
    <NumberInitialWorkpieces>80</NumberInitialWorkpieces>
    <MaxProcessingTimeInMinutes>10</MaxProcessingTimeInMinutes>
    <MaxProcessingExtraTimeAllowance>0.1</MaxProcessingExtraTimeAllowance>
    <!--The extra time allowance of the termination phase is defined as a percentage over the MaxProcessingTime. For example, if the extra time allowance is 0.5, then 50% of the MaxProcessingTime is considered as extra time.-->
    <MinAdmissibleImprovementAtExtraTime>0.00001</MinAdmissibleImprovementAtExtraTime>
    <!--This is the minimum improvement (as a percentage over the current cost) that a candidate solution need to reach in order to continue processing.-->
    <MaxProcessingTimeLocalOptInSeconds>5</MaxProcessingTimeLocalOptInSeconds>
    <MaxNumberProcessedWorkpieces>10000</MaxNumberProcessedWorkpieces>
    <MaxNumberReservedWorkpieces>150</MaxNumberReservedWorkpieces>
    <!--it has been noted that the best performance is observed when BetterReplyModeProcessingSteps=70, and BetterReplyModeInternalSteps=7. However, under Power-Loss Constraints, it might be better to decrease those numbers, and instead increase either the size of the pool or the processing time.-->
  </OptimizationParameters>
  <FilterParameters>
    <GoodStandingThresholdRate>-2e-8</GoodStandingThresholdRate>
    <GoodStandingRateInterval>150</GoodStandingRateInterval>
    <GoodStandingFixedRate>0</GoodStandingFixedRate>
    <!--GoodStandingFixedRate=1 corresponds to using a fixed denominator in the calculation of the Good Standing derivative (that is: GoodStandingRateInterval * Performance(0), where the second part normalizes with respect to the performance). Instead if GoodStandingFixedRate=0, then the denominator in the calculation of the GoodStanding derivative is num_processing_steps * performance(0), i.e., we use the total number of processing steps.-->
    <LastImprovementThresholdInterval>200</LastImprovementThresholdInterval>
    <HighPotentialThresholdRate>-1e-5</HighPotentialThresholdRate>
    <HighPotentialRateInterval>5</HighPotentialRateInterval>
    <HighPotentialFixedRate>0</HighPotentialFixedRate>
    <AddReservationThreshold>0.2</AddReservationThreshold>
    <KeepProcessingThreshold>0.4</KeepProcessingThreshold>
    <KeepProcessingThresholdStartStep>100</KeepProcessingThresholdStartStep>
  </FilterParameters>
  <InitializationAlgorithms>
    <FFD_1_1>1</FFD_1_1>
    <FFD_1_2>1</FFD_1_2>
    <FFD_1_3>1</FFD_1_3>
    <FFD_1_4>0</FFD_1_4>
    <FFD_1_5>0</FFD_1_5>
    <FFD_2_1>1</FFD_2_1>
    <FFD_2_2>1</FFD_2_2>
    <FFD_3_1>1</FFD_3_1>
    <FFD_3_2>1</FFD_3_2>
  </InitializationAlgorithms>
  <Operations>
    <Move>1</Move>
    <CoilSwitch>1</CoilSwitch>
    <Swap>1</Swap>
    <Split>0</Split>
    <RedUebl>0</RedUebl>
    <RemCoi>1</RemCoi>
    <RemCoiRev>1</RemCoiRev>
    <Remove>0</Remove>
    <MultipleStepSwap>0</MultipleStepSwap>
    <HigherLevelSwap>0</HigherLevelSwap>
  </Operations>
  <Terminology>
    <CoilParameters>
      <SheetName>Band</SheetName>
      <CoilName>Coil</CoilName>
      <MaterialQualityName>QUALITY</MaterialQualityName>
      <ManufacturerName>LIEFERANT</ManufacturerName>
      <ComponentWeightName>WEIGHT</ComponentWeightName>
    </CoilParameters>
    <MaterialParameters>
      <SpecificWeightName>specific weight</SpecificWeightName>
      <MaterialCategoryName>mat_cat</MaterialCategoryName>
    </MaterialParameters>
    <PowerLossParameters>
      <beta_SST>beta_SST</beta_SST>
      <YokeMainLimbProportion>YokeMainLimbProportion</YokeMainLimbProportion>
      <ReturnLimbMainLimbProportion>ReturnLimbMainLimbProportion</ReturnLimbMainLimbProportion>
      <Theta_Kern>Theta_Kern</Theta_Kern>
      <D_Kern>D_Kern</D_Kern>
      <L_Kern>L_Kern</L_Kern>
      <E_Kern>E_Kern</E_Kern>
      <e1_Kern>e1_Kern</e1_Kern>
      <Impact>Impact</Impact>
      <Frequency>Freq</Frequency>
      <m_Kern>m_Kern</m_Kern>
      <Quality>QUALITY</Quality>
      <B_Kern>B_Kern</B_Kern>
      <PowerLossCoefficient>Coil_LossesAt17T50f_Manufacturer</PowerLossCoefficient>
      <MaxPowerLoss>L_max</MaxPowerLoss>
    </PowerLossParameters>
    <GlobalRunParameters>
      <RestWidths>RestWidths</RestWidths>
      <Use_R_min>Use_R_min</Use_R_min>
      <Use_S_max>Use_S_max</Use_S_max>
      <N_re-cutting>N_re_cutting</N_re-cutting>
      <ObjectiveFunction>ObjectiveFunction</ObjectiveFunction>
      <SplitLargeResidualSheets>Split_Large_ResidualSheets</SplitLargeResidualSheets>
      <SlitAllCoils>Slit_All_Coils</SlitAllCoils>
      <StartWidthLargeResidualSheets>StartWidth_Large_ResidualSheets</StartWidthLargeResidualSheets>
    </GlobalRunParameters>
    <Weights>
      <omega_coil_weight>omega_coil_weight</omega_coil_weight>
      <omega_rbwidth>omega_rbwidth</omega_rbwidth>
      <omega_seam>omega_seam</omega_seam>
      <omega_lcutting>omega_lcutting</omega_lcutting>
      <omega_cuttertypechange>omega_cuttertypechange</omega_cuttertypechange>
      <omega_clamping>omega_clamping</omega_clamping>
      <omega_removing>omega_removing</omega_removing>
      <omega_adjustment>omega_adjustment</omega_adjustment>
      <omega_power_loss>omega_power_loss</omega_power_loss>
      <omega_maintenance>omega_maintenance</omega_maintenance>
      <omega_noise_loss>omega_noise_loss</omega_noise_loss>
      <omega_sheets_used_as_coils>omega_sheets_used_as_coils</omega_sheets_used_as_coils>
      <omega_large_residual_sheets>omega_large_residual_sheets</omega_large_residual_sheets>
      <omega_use_priority>omega_use_priority</omega_use_priority>
    </Weights>
    <AvailableWorkpieceProperties>
      <UseAsCoil>UseAsCoil</UseAsCoil>
      <UsePriority>USE</UsePriority>
    </AvailableWorkpieceProperties>
    <Observations>
      <Features>
        <PowerLossCoefficient_17_50>PskAvg_17_50</PowerLossCoefficient_17_50>
        <PowerLossCoefficient_17_50_Parameters>Fset,Bset</PowerLossCoefficient_17_50_Parameters>
      </Features>
      <MeasuringParameters>
        <B_Kern>Bset</B_Kern>
        <Freq>Fset</Freq>
      </MeasuringParameters>
    </Observations>
  </Terminology>
  <OtherParameters>
    <OverlengthWasteThreshold>20000</OverlengthWasteThreshold>
    <IncludePowerLossCoilMeasurements>1</IncludePowerLossCoilMeasurements>
    <RemoveLogStreams>0</RemoveLogStreams>
    <RunningFinalTests>1</RunningFinalTests>
    <PostMinimizePLdeviation>1</PostMinimizePLdeviation>
    <PostMinimizationMaxSwaps>20</PostMinimizationMaxSwaps>
    <PostMinimizationDeviationThreshold>0.05</PostMinimizationDeviationThreshold>
  </OtherParameters>
</Inputs>
